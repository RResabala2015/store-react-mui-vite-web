{
  "name": "store-react-ts-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "proxy": "http://localhost:8085",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production --outDir dist/client",
    "build:client": "vite build --mode production --outDir dist/client",
    "build:server": "vite build --mode production --outDir dist/server --ssr src/main.tsx",
    "start": "vite --port 3000 --host",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint --ext .js,.cjs,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.cjs,.ts,.tsx . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.16.4",
    "@mui/material": "^5.16.4",
    "@mui/styled-engine-sc": "^6.0.0-alpha.18",
    "@mui/x-charts": "^7.10.0",
    "@mui/x-data-grid": "^7.15.0",
    "axios": "^1.7.5",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.0",
    "react-ts-app": "file:",
    "store-react-ts-app": "file:"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "license": "MIT"
}
